{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/node",
            "config": { "zeroConfig": true }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "index.html" 
        }
    ],

    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
}